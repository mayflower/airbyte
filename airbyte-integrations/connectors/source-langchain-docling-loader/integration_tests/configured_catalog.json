{
  "streams": [
    {
      "stream": {
        "name": "docling_documents",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "content": {
              "type": "string"
            },
            "metadata": {
              "type": "object",
              "additionalProperties": true
            },
            "extracted_at": {
              "type": "string",
              "format": "date-time"
            }
          }
        },
        "supported_sync_modes": ["full_refresh"]
      },
      "sync_mode": "full_refresh",
      "destination_sync_mode": "overwrite"
    }
  ]
}
