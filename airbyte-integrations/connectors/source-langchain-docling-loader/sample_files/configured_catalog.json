{
  "streams": [
    {
      "stream": {
        "name": "docling_documents",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "generationId": {
              "type": "string",
              "description": "A unique identifier for the document chunk"
            },
            "content": {
              "type": "string",
              "description": "The text content of the document or document chunk"
            },
            "metadata": {
              "type": "object",
              "description": "Metadata associated with the document",
              "properties": {
                "source_file": {
                  "type": "string",
                  "description": "Absolute path to the source file"
                },
                "relative_path": {
                  "type": "string",
                  "description": "Path relative to the input directory"
                }
              },
              "additionalProperties": true
            },
            "extracted_at": {
              "type": "string",
              "format": "date-time",
              "description": "Timestamp when the document was extracted"
            },
            "lastModified": {
              "type": "string",
              "format": "date-time",
              "description": "Last modification timestamp of the source file"
            }
          }
        },
        "supported_sync_modes": ["full_refresh", "incremental"],
        "source_defined_primary_key": [["id"]],
        "source_defined_cursor": true,
        "default_cursor_field": ["file_modified_at"]
      },
      "sync_mode": "full_refresh",
      "destination_sync_mode": "overwrite",
      "primary_key": [["id"]],
      "cursor_field": ["file_modified_at"]
    }
  ]
}
